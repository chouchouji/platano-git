{
  "name": "platano-git",
  "version": "0.2.4",
  "description": "A tool for simplifying git operations",
  "type": "module",
  "bin": {
    "platano": "/dist/bin.js"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./node_modules",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "release": "pnpm build && npx vr release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chouchouji/platano-git"
  },
  "homepage": "https://github.com/chouchouji/platano-git",
  "keywords": [
    "cli",
    "nodejs",
    "git"
  ],
  "files": [
    "dist",
    "README.zh-CN.md"
  ],
  "author": "chouchouji <1305974212@qq.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "tinyexec": "^0.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@varlet/release": "^0.2.11",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.0"
  }
